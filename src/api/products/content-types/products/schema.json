{
  "kind": "collectionType",
  "collectionName": "productsp",
  "info": {
    "singularName": "products",
    "pluralName": "productsp",
    "displayName": "products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "price": {
      "type": "float",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "volume": {
      "type": "float"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categories.categories",
      "inversedBy": "products"
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subcategoriess.subcategoriess",
      "inversedBy": "products"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brands.brands",
      "inversedBy": "products"
    },
    "desc": {
      "type": "text"
    }
  }
}
